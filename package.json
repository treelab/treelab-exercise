{
  "name": "treelab-landing",
  "version": "1.0.0",
  "description": "Treelab landing NextJs + Typescript",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon server.js",
    "build": "next build",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node server.js",
    "generate": "nodemon --watch graphql --ignore 'graphql/**/types/*.ts' -e ts,tsx --exec yarn run apollo codegen:generate --target=typescript --globalTypesFile=./types/graphql-global-types.ts types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caminerinc/treelab-client.git"
  },
  "author": "Caminer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/caminerinc/treelab-client/issues"
  },
  "homepage": "https://github.com/caminerinc/treelab-client#readme",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@types/react-select": "^3.0.0",
    "@types/styled-components": "^4.1.15",
    "@zeit/next-css": "^1.0.1",
    "apollo-boost": "^0.4.4",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "cookie": "^0.4.0",
    "express": "^4.16.4",
    "graphql": "^14.5.8",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.3.2",
    "next-routes": "^1.4.2",
    "next-runtime-dotenv": "^1.2.0",
    "next-with-apollo": "^4.3.0",
    "react": "^16.8.4",
    "react-code-input": "^3.8.1",
    "react-dom": "^16.8.4",
    "styled-components": "^4.2.0",
    "yarn": "^1.17.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@types/next": "7.0.6",
    "@types/no-scroll": "^2.1.0",
    "@types/node": "^11.11.3",
    "@types/react-dom": "^16.8.3",
    "@zeit/next-typescript": "^1.1.1",
    "apollo": "^2.21.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-define": "^1.3.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "nodemon": "^1.18.6",
    "prettier": "1.15.1",
    "pretty-quick": "^1.8.0",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
